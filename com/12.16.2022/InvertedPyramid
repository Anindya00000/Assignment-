//InvertedPyramid

package com;

public class InvertedPyramid 
{
	public static void main(String[] args)
	{
          for(int k=0;k<5;k++)
          {
               for(int j=1;j<=k;j++)
  	       {
  		     System.out.print(" ");  
  	       }
               for(int i=1;i<=5-k;i++)
	       {
		     System.out.print("* ");
	       }
	       System.out.println();
          }
       }
}
