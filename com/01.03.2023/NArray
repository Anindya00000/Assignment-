package com;

public class NArray //Main class where 'main method' exists 
{
	 public static void main(String []args)
	   {
		   int arr[]={4,-5,6,-8,7};      //create array
		   System.out.println("Negative Numbers Present In The Array: ");
		   for(int i=0;i<arr.length;i++)
		   {
			   if(arr[i]<0)
				    System.out.print(arr[i]+" ");
		   }
	   }
}
